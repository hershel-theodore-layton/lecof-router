{
  "name": "hershel-theodore-layton/lecof-router",
  "description": "A request router for Hack and HHVM without a build step.",
  "license": "MIT",
  "require": {
    "hhvm": "^4.73",
    "hhvm/hsl": "^4.41",
    "hershel-theodore-layton/lecof-router-interfaces": "^0.2"
  },
  "require-dev": {
    "hhvm/hhvm-autoload": "^3.2",
    "hhvm/hhast": "^4.102",
    "hhvm/hacktest": "^2.3",
    "facebook/fbexpect": "^2.8"
  },
  "scripts": {
    "post-install-cmd": [
      "rm vendor/autoload.php vendor/composer/*.php"
    ],
    "post-update-cmd": [
      "rm vendor/autoload.php vendor/composer/*.php"
    ]
  }
}